{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "aman00323/emailchecker": "^2.2",
        "anandsiddharth/laravel-paytm-wallet": "^2.0",
        "barryvdh/laravel-dompdf": "^0.9.0",
        "brunocfalcao/blade-feather-icons": "^2.0.1",
        "doctrine/dbal": "^2.9.0",
        "fideloper/proxy": "^4.4.1",
        "fruitcake/laravel-cors": "^2.0.3",
        "gathuku/laravelmpesa": "^1.2",
        "guzzlehttp/guzzle": "^7.2",
        "infureal/artisan-gui": "^2.0",
        "instamojo/instamojo-php": "^1.0",
        "iwasherefirst2/laravel-multimail": "^1.3",
        "iyzico/iyzipay-php": "^2.0",
        "jdavidbakr/mail-tracker": "^4.0",
        "kingflamez/laravelrave": "^4.2",
        "laravel/framework": "^8.14.0",
        "laravel/tinker": "^2.5.0",
        "laravel/ui": "^3.1",
        "laravolt/avatar": "^4.0",
        "maatwebsite/excel": "^3.1",
        "mckenziearts/laravel-notify": "^2.2",
        "mollie/laravel-mollie": "^2.19",
        "nexmo/laravel": "^2.4",
        "paypal/rest-api-sdk-php": "^1.14",
        "plivo/plivo-php": "^4.17",
        "razorpay/razorpay": "^2.8",
        "realrashid/sweet-alert": "^3.2",
        "romanzipp/laravel-queue-monitor": "^2.0",
        "sendgrid/sendgrid": "^7.11",
        "sentry/sentry-laravel": "^2.3",
        "spatie/laravel-mailable-test": "^2.2.3",
        "stripe/stripe-php": "^7.67",
        "sumup/sumup-ecom-php-sdk": "^1.1",
        "twilio/sdk": "^6.16.0",
        "tzsk/payu": "^6.0",
        "unicodeveloper/laravel-paystack": "^1.0"
    },
    "require-dev": {
        "facade/ignition": "^2.5.6",
        "laravel/pint": "^1.0",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.1.0",
        "phpunit/phpunit": "^9.4.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
        "app/Helpers.php",
        "app/EmailVerify.php"
        ],
        "classmap": [
            "database/seeders",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
